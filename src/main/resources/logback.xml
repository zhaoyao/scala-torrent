<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <target>System.out</target>
        <encoder>
            <pattern>%-5level %date{yyyy-MM-dd'T'HH:mm:ss.SSS}%logger{1} - %msg%n</pattern>
        </encoder>
    </appender>

    <!--<logger name="storrent" level="DEBUG"/>-->

    <root level="INFO">
        <appender-ref ref="CONSOLE"/>
    </root>

</configuration>